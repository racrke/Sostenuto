func fibo :: Number lim -> Number 
|>	var(Number ret)
	if :: lim == 0 -> ret = 0;
	if :: lim == 1 -> ret = 1;
	if :: lim > 1 -> ret = fibo(lim - 1) + fibo(lim - 2);

	return ret;
<|


